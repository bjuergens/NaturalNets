{
    "number_generations": 5,
    "number_validation_runs": 20,
    "number_rounds": 5,
    "maximum_env_seed": 100000,
    "environment": {
        "type": "ReacherMemory",
        "frames_observation_phase": 20,
        "frames_memory_phase": 20,
        "frames_action_phase": 50
    },
    "brain": {
        "type": "LSTMNN",
        "hidden_layer_structure": [[10]],
        "diagonal_hidden_to_hidden":  true,
        "use_bias": false
    },
    "optimizer": {
        "type": "CMA-ES-Deap",
        "population_size": 10,
        "sigma": 1.0
    }
}