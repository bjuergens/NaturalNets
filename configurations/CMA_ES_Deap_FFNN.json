{
    "number_generations": 500,
    "number_validation_runs": 100,
    "number_rounds": 5,
    "maximum_env_seed": 100000,
    "environment": {
        "type": "CollectPoints",
        "maze_columns": 5,
        "maze_rows": 5,
        "maze_cell_size": 80,
        "agent_radius": 12,
        "point_radius": 8,
        "agent_movement_range": 10.0,
        "reward_per_collected_point": 500.0,
        "reward_per_collected_negative_point": -700.0,
        "number_time_steps": 1000
    },
    "brain": {
        "type": "FFNN",
        "hidden_layers": [32, 32, 16],
        "neuron_activation": "tanh",
        "neuron_activation_output": "tanh",
        "use_bias": true
    },
    "optimizer": {
        "type": "CMA-ES-Deap",
        "population_size": 112,
        "sigma": 1.0
    }
}