{
  "number_generations": 100,
  "number_validation_runs": 5,
  "number_rounds": 10,
  "maximum_env_seed": 100000,
  "environment": {
    "type": "CollectPoints",
    "maze_columns": 5,
    "maze_rows": 5,
    "maze_cell_size": 80,
    "agent_radius": 12,
    "point_radius": 8,
    "agent_movement_range": 10.0,
    "reward_per_collected_positive_point": 500.0,
    "reward_per_collected_negative_point": -700.0,
    "number_time_steps": 1000
  },
  "brain": {
    "type": "FFNN",
    "hidden_layers": [
      16,
      16
    ],
    "neuron_activation": "relu",
    "neuron_activation_output": "relu",
    "use_bias": true
  },
  "optimizer": {
    "type": "OpenAI-ES",
    "population_size": 100,
    "learning_rate": 0.005,
    "sigma": 1.0
  }
}