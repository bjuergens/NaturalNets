{
    "environment": "procgen:procgen-heist-v0",
    "number_generations": 5,
    "number_validation_runs": 100,
    "number_rounds": 5,
    "maximum_env_seed": 100000,
    "brain": {
        "type": "Indirect-CTRNN",
        "ffnn_config": {
          "type": "FeedForward",
          "use_bias": true,
          "hidden_layers": [16, 32, 16, 8],
          "neuron_activation": "tanh",
          "neuron_activation_output": "linear"
        },
        "ctrnn_config": {
          "type": "CTRNN",
          "delta_t": 0.05,
          "number_neurons": 200,
          "v_mask": "random",
          "v_mask_density": 0.0005,
          "w_mask": "dense",
          "t_mask": "random",
          "t_mask_density": 0.1,
          "clipping_range_max": 1.0,
          "clipping_range_min": -1.0
        }
    },
    "optimizer": {
        "type": "CMA-ES-Deap",
        "population_size": 112,
        "sigma": 1.0
    }
}