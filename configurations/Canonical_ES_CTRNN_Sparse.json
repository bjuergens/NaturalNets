        "t_mask_param": 0.1,
        "clipping_range_min": -1.0,
        "clipping_range_max": 1.0
    },
    "optimizer": {
        "type": "Canonical-ES",
        "offspring_population_size": 20,
        "parent_population_size": 10,
        "mutation_step_size": 0.05
    }
}
